{
  "name": "sidgautamscript-devtools",
  "displayName": "SidGautamScript DevTools",
  "author": "Gautam Batta",
  "publisher": "Gautam Batta",
  "icon": "./assets/icon.png",
  "description": "A VS Code extension for developing with SidGautamScript",
  "version": "1.2.0",
  "main": "./extension.js",
  "activationEvents": [
    "onLanguage"
  ],
  "keywords": [
    "sidg",
    "sgscript",
    "sidgautamscript",
    "devtools",
    "formatters"
  ],
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Programming Languages",
    "Formatters"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sgscript",
        "aliases": [
          "SidGautamScript",
          "sgscript"
        ],
        "extensions": [
          ".sidg"
        ],
        "icon": {
          "light": "./assets/source-icon-light.svg",
          "dark": "./assets/source-icon.svg"
        },
        "configuration": "./language-configuration.json"
      },
      {
        "id": "sgscript-compiled",
        "aliases": [
          "SidGautamScript-Compiled",
          "sgscript-compiled"
        ],
        "extensions": [
          ".sidgc"
        ],
        "icon": {
          "light": "./assets/compiled-icon-light.svg",
          "dark": "./assets/compiled-icon.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "sgscript",
        "scopeName": "source.sidg",
        "path": "./syntaxes/sgscript.tmLanguage.json"
      }
    ],
    "formatters": [
      {
        "language": "sgscript",
        "scheme": "file",
        "documentSelector": [
          {
            "language": "sgscript"
          }
        ]
      }
    ]
  }
}